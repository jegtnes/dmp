<h1><a href="/"></a>Project Brunel</h1>

<h2 class="tk-tamarillo">You are embarking on a delightful journey from <strong>Filton Abbey Wood</strong> to <strong>Cardiff Central</strong></h2>
<table>
  <tr>
    <th>Departure</th>
    <th>Arrival</th>
    <th>Duration</th>
    <th>Changes</th>
    <th>Price</th>
  </tr>

  <% @journeys.each do |journey| %>
    <tr>
      <td><%= journey["dep"] %></td>
      <td><%= journey["arr"] %></td>
      <td><%= journey["dur"] %></td>
      <td><%= journey["chg"] %></td>
      <td><%= journey["fare"] %></td>
    </tr>
  <% end %>
</table>
